<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.faragz_bot">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <queries>
        <package android:name="com.google.android.youtube" />
        <package android:name="com.google.android.calendar" />
        <package android:name="com.facebook.katana" />
        <package android:name="com.sec.android.app.popupcalculator" />
        <package android:name="com.whatsapp" />
        <package android:name="com.instagram.android" />
        <package android:name="com.zhiliaoapp.musically" />
        <package android:name="com.twitter.android" />
        <package android:name="com.snapchat.android" />



    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/codezic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/codezic_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Faragz_Bot">
        <activity
            android:name=".AddActivity"
            android:label="Add Note"
            android:parentActivityName=".Reminder"
            android:exported="false" />
        <activity
            android:name=".logo"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Reminder"
            android:exported="false"
            android:label="Reminder" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"></activity>
    </application>

</manifest>